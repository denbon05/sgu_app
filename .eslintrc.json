{
		"root": true,
    "env": {
				"browser": true,
				"node": true,
        "jest": true
		},
    "extends": [
				"airbnb-base",
				"plugin:jest/recommended"
		],
		"parser": "babel-eslint",
    "rules": {
			"no-console": 0,
			"import/extensions": 0,
			"new-cap": ["error", { "newIsCap": false }],
			"no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"]
    }
}
