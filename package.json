{
  "name": "sgu_app",
  "version": "0.1.0",
  "description": "test sgu",
  "main": "server/index.js",
  "scripts": {
    "test": "jest --colors --silent",
    "prebuild": "rm -rf dist",
    "build-db": "cp knexfile.js dist && knex migrate:latest",
    "seed-db": "npx knex seed:run",
    "build-configs": "babel webpack.config.babel.js --out-dir dist",
    "build-server": " babel server --out-dir dist/server --source-maps inline && cp -r server/views dist/server/views",
    "build-frontend": "webpack --mode production",
    "build": "npm run build-configs && npm run build-frontend && npm run build-server && npm run build-db && npm run seed-db",
    "start": "node dist/server/bin/server.js"
  },
  "engines": {
    "node": "14.x"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/helpers/"
    ]
  },
  "keywords": [
    "gsu",
    "fastify",
    "node",
    "js"
  ],
  "dependencies": {
    "bootstrap": "^4.6.0",
    "debug": "^4.3.1",
    "fastify": "^3.11.0",
    "fastify-auth": "^1.1.0",
    "fastify-error-page": "^4.0.0",
    "fastify-formbody": "^5.0.0",
    "fastify-method-override": "^1.5.4",
    "fastify-objectionjs": "^0.3.0",
    "fastify-passport": "^0.4.1",
    "fastify-reverse-routes": "^2.0.0",
    "fastify-secure-session": "^2.3.1",
    "fastify-sensible": "^3.1.0",
    "fastify-static": "^3.4.0",
    "i18next": "^19.8.7",
    "jquery": "^3.6.0",
    "knex": "^0.21.17",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.5",
    "objection": "^2.2.14",
    "objection-unique": "^1.2.1",
    "pg": "^8.5.1",
    "pino-pretty": "^4.5.0",
    "point-of-view": "^4.12.0",
    "pug": "^3.0.0",
    "qs": "^6.9.6",
    "sqlite3": "^5.0.0",
    "webpack": "^5.21.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@jest/globals": "^26.6.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "nodemon": "^2.0.7",
    "style-loader": "^2.0.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "author": "Denys Bondarenko",
  "license": "MIT"
}
